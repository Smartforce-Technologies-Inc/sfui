{
  "name": "sfui",
  "version": "1.7.2",
  "description": "SmartForce UI Library",
  "author": "SmartForce Technologies, Inc.",
  "license": "MIT",
  "repository": "AdventosCorp/sfui",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "run-s clean && microbundle-crl --no-compress --format modern,cjs && npm run copy-styles",
    "start": "run-s clean && microbundle-crl watch --no-compress --format modern,cjs && npm run copy-styles",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "npm run build-storybook",
    "deploy": "gh-pages -d storybook-static",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "copy-styles": "cpx \"src/**/*.scss\" dist/Styles"
  },
  "peerDependencies": {
    "react": "^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.16",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/autosuggest-highlight": "^3.1.1",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.178",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^12.12.38",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.2",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "icomoon-react": "^2.0.19",
    "microbundle-crl": "^0.13.10",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^16.13.1",
    "react-scripts": "^4.0.0",
    "sass-loader": "^10.0.1",
    "style-loader": "^1.2.1",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@date-io/moment": "^1.3.13",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/material": "^5.11.15",
    "@types/google.maps": "^3.48.3",
    "@types/resize-observer-browser": "^0.1.5",
    "autosuggest-highlight": "^3.1.1",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.29.1",
    "react-number-format": "^4.6.3"
  }
}
